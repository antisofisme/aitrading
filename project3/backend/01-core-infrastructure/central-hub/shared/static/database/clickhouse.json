{
  "connection": {
    "host": "${CLICKHOUSE_HOST}",
    "http_port": "${CLICKHOUSE_PORT}",
    "native_port": 9000,
    "database": "${CLICKHOUSE_DB}",
    "user": "${CLICKHOUSE_USER}",
    "password": "${CLICKHOUSE_PASSWORD}",
    "secure": false
  },
  "settings": {
    "max_query_size": 262144000,
    "max_ast_elements": 200000,
    "max_expanded_ast_elements": 500000,
    "readonly": 0,
    "allow_ddl": 1
  },
  "pool": {
    "max_connections": 10,
    "connection_timeout": 10000,
    "receive_timeout": 300000,
    "send_timeout": 300000
  },
  "tables": {
    "analytics": ["trading_metrics", "market_analysis", "performance_data"],
    "logs": ["error_logs", "audit_logs", "system_logs"]
  }
}