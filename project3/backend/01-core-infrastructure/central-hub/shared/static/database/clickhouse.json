{
  "connection": {
    "host": "ENV:CLICKHOUSE_HOST",
    "http_port": "ENV:CLICKHOUSE_PORT",
    "native_port": 9000,
    "database": "ENV:CLICKHOUSE_DB",
    "user": "ENV:CLICKHOUSE_USER",
    "password": "ENV:CLICKHOUSE_PASSWORD",
    "secure": false
  },
  "settings": {
    "max_query_size": 262144000,
    "max_ast_elements": 200000,
    "max_expanded_ast_elements": 500000,
    "readonly": 0,
    "allow_ddl": 1
  },
  "pool": {
    "max_connections": 10,
    "connection_timeout": 10000,
    "receive_timeout": 300000,
    "send_timeout": 300000
  },
  "tables": {
    "analytics": ["trading_metrics", "market_analysis", "performance_data"],
    "logs": ["error_logs", "audit_logs", "system_logs"]
  }
}