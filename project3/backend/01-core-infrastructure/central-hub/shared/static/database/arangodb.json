{
  "connection": {
    "host": "suho-arangodb",
    "port": 8529,
    "protocol": "http",
    "username": "root",
    "password": "${ARANGO_ROOT_PASSWORD}",
    "database": "_system"
  },
  "authentication": {
    "enabled": true,
    "jwt_secret": "${ARANGO_JWT_SECRET}",
    "timeout": 3600
  },
  "databases": {
    "trading": {
      "collections": {
        "vertices": ["users", "strategies", "markets", "instruments"],
        "edges": ["user_strategies", "strategy_markets", "market_instruments"]
      }
    },
    "analytics": {
      "collections": {
        "vertices": ["analysis_nodes", "performance_nodes", "risk_nodes"],
        "edges": ["analysis_links", "performance_links", "risk_links"]
      }
    }
  },
  "settings": {
    "log_level": "info",
    "max_connections": 50,
    "connection_timeout": 30000,
    "query_timeout": 60000
  },
  "graph_settings": {
    "default_replication_factor": 1,
    "write_concern": 1,
    "sync_replication": false
  }
}