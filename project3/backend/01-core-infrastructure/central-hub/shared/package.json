{
  "name": "@suho/shared-components",
  "version": "2.0.0",
  "description": "Shared components, protocols, and transfer layer for Suho AI Trading System",
  "main": "index.js",
  "types": "generated/nodejs/index.d.ts",
  "files": [
    "transfer/**/*",
    "logging/**/*",
    "proto/**/*.proto",
    "generated/nodejs/**/*",
    "protobuf/**/*.proto",
    "suho-binary/specs/**/*",
    "utils/**/*",
    "config/**/*"
  ],
  "scripts": {
    "generate": "npm run generate:nodejs && npm run generate:python && npm run generate:go",
    "generate:nodejs": "protoc --js_out=import_style=commonjs,binary:generated/nodejs --ts_out=generated/nodejs protobuf/*.proto",
    "generate:python": "python -m grpc_tools.protoc --python_out=generated/python --pyi_out=generated/python protobuf/*.proto",
    "generate:go": "protoc --go_out=generated/go --go_opt=paths=source_relative protobuf/*.proto",
    "build": "npm run generate && npm run build:nodejs",
    "build:nodejs": "tsc -p tsconfig.json",
    "test": "jest",
    "lint": "eslint generated/nodejs/**/*.ts",
    "clean": "rimraf generated/*/",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "protobufjs": "^7.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "google-protobuf": "^3.21.0",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "axios": "^1.5.0",
    "kafkajs": "^2.2.4",
    "nats": "^2.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "protobuf",
    "binary-protocol",
    "trading",
    "mt5",
    "suho",
    "shared-protocols",
    "transfer-layer",
    "nats",
    "kafka",
    "grpc",
    "shared-components"
  ],
  "author": "Suho AI Trading Team",
  "license": "PRIVATE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suho/trading-protocols.git"
  }
}