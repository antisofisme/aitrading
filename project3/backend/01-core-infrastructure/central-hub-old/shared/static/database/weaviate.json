{
  "connection": {
    "host": "suho-weaviate",
    "http_port": 8080,
    "grpc_port": 50051,
    "scheme": "http",
    "timeout": 30000
  },
  "authentication": {
    "type": "anonymous",
    "enabled": true
  },
  "settings": {
    "query_defaults_limit": 25,
    "persistence_data_path": "/var/lib/weaviate",
    "default_vectorizer_module": "none",
    "log_level": "info"
  },
  "classes": {
    "trading": ["TradingStrategy", "MarketSignal", "TradingResult"],
    "analytics": ["MarketAnalysis", "PerformanceMetric", "RiskAssessment"],
    "knowledge": ["TradingKnowledge", "MarketInsight", "StrategyPattern"]
  },
  "vectors": {
    "dimensions": 768,
    "distance_metric": "cosine",
    "ef_construction": 128,
    "max_connections": 64
  }
}