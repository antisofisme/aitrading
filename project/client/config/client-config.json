{
  "mt5": {
    "server": "",
    "login": "",
    "password": "",
    "maxConnections": 5,
    "targetTicksPerSecond": 25,
    "symbols": [
      "EURUSD",
      "GBPUSD",
      "USDJPY",
      "USDCHF",
      "AUDUSD"
    ],
    "timeframes": [
      "M1",
      "M5",
      "M15",
      "H1"
