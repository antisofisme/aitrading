{
  "name": "ai-chain-analytics",
  "version": "1.0.0",
  "description": "AI-powered chain analysis system for predictive failure detection and pattern recognition",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "start": "node dist/src/index.js",
    "dev": "ts-node src/index.ts",
    "clean": "rimraf dist",
    "docs": "typedoc src --out docs"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "anomaly-detection",
    "pattern-recognition",
    "predictive-analysis",
    "real-time",
    "streaming",
    "graph-neural-networks",
    "time-series",
    "chain-analysis"
  ],
  "author": "AI Trading System",
  "license": "PROPRIETARY",
  "dependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.25.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "testTimeout": 30000,
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neliti/aitrading.git",
    "directory": "project/backend/ai-chain-analytics"
  },
  "bugs": {
    "url": "https://github.com/neliti/aitrading/issues"
  },
  "homepage": "https://github.com/neliti/aitrading#readme"
}