{
  "name": "aitrading-backend",
  "version": "1.0.0",
  "description": "AI Trading Platform Backend - Level 1 Foundation Complete",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "dev": "docker-compose -f docker-compose.dev.yml up",
    "logs": "docker-compose logs -f",
    "health": "node scripts/test-health-checks.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testMatch='**/integration/**/*.test.js'",
    "build": "npm run test && npm run health",
    "validate-level1": "npm run health && echo 'Level 1 Foundation validation complete'",
    "api-gateway": "cd api-gateway && npm start",
    "central-hub": "cd central-hub && node src/server.js",
    "database-service": "cd database-service && node src/server.js",
    "data-bridge": "cd data-bridge && node src/server.js"
  },
  "keywords": [
    "ai-trading",
    "microservices",
    "level-1-foundation",
    "plan2-implementation",
    "multi-tenant",
    "business-api"
  ],
  "author": "AI Trading Platform Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "level1Status": {
    "foundation": "COMPLETE",
    "businessAPI": "IMPLEMENTED",
    "multiTenant": "PREPARED",
    "serviceOrchestration": "OPERATIONAL",
    "multiDatabase": "CONNECTED",
    "mt5Integration": "ENHANCED",
    "readyForLevel2": true,
    "completedDate": "2024-09-22",
    "validationScript": "npm run validate-level1"
  }
}