{
  "name": "@aitrading/api-gateway",
  "version": "1.0.0",
  "description": "Enhanced API Gateway service for AI Trading Platform - Multi-database architecture with Flow Registry",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:enhanced": "node src/new-enhanced-server.js",
    "start:lightweight": "node src/lightweight-gateway.js",
    "dev": "nodemon src/server.js",
    "dev:enhanced": "nodemon src/new-enhanced-server.js",
    "dev:lightweight": "nodemon src/lightweight-gateway.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "health-check": "curl -f http://localhost:3001/health || exit 1",
    "db-check": "curl -f http://localhost:3001/api/v1/status/databases || exit 1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "@clickhouse/client": "^0.2.5",
    "weaviate-ts-client": "^1.4.0",
    "arangojs": "^8.8.1",
    "joi": "^17.11.0",
    "express-slow-down": "^1.6.0",
    "node-cron": "^3.0.3",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "api-gateway",
    "microservices",
    "express",
    "trading",
    "ai",
    "multi-database",
    "dragonfly",
    "clickhouse",
    "weaviate",
    "arangodb",
    "flow-registry"
  ],
  "private": true,
  "author": "AI Trading Platform Team",
  "license": "MIT"
}