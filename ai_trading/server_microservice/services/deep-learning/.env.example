# Deep Learning Service Environment Variables
# Copy this file to .env and set your actual values

# Service Configuration
MICROSERVICE_ENVIRONMENT=development
DEEP_LEARNING_PORT=8004
DEEP_LEARNING_DEBUG=false
DEEP_LEARNING_HOST=0.0.0.0

# CORS Configuration
CORS_ENABLED=true
CORS_ORIGINS=*

# Rate Limiting
RATE_LIMITING_ENABLED=true
RATE_LIMITING_REQUESTS_PER_MINUTE=1000

# Deep Learning Framework Configuration
DL_FRAMEWORKS=tensorflow,pytorch,transformers
DL_GPU_ENABLED=false
DL_MODEL_CACHE_SIZE_GB=10
DL_INFERENCE_TIMEOUT_SECONDS=120

# Model Configuration - Transformers
MODELS_TRANSFORMERS_ENABLED=true
MODELS_TRANSFORMERS_MAX_SEQUENCE_LENGTH=512
MODELS_TRANSFORMERS_BATCH_SIZE=8
MODELS_TRANSFORMERS_MODEL_NAME=bert-base-uncased

# Model Configuration - Neural Networks
MODELS_NEURAL_NETWORKS_ENABLED=true
MODELS_NEURAL_NETWORKS_LAYERS=dense,lstm,cnn

# Model Configuration - Pattern Discovery
MODELS_PATTERN_DISCOVERY_ENABLED=true
MODELS_PATTERN_DISCOVERY_ALGORITHMS=clustering,anomaly_detection,time_series_analysis

# AI Coordination Configuration
AI_COORDINATION_MULTI_MODEL_ENSEMBLE=true
AI_COORDINATION_MODEL_SELECTION_STRATEGY=performance
AI_COORDINATION_FALLBACK_ENABLED=true

# Performance Configuration
PERFORMANCE_PARALLEL_PROCESSING=true
PERFORMANCE_MEMORY_LIMIT_GB=16
PERFORMANCE_CPU_CORES=8
PERFORMANCE_MODEL_WARMING=true

# Monitoring Configuration
MONITORING_MODEL_PERFORMANCE_TRACKING=true
MONITORING_PREDICTION_LOGGING=true
MONITORING_RESOURCE_MONITORING=true
HEALTH_CHECK_INTERVAL=30
METRICS_ENABLED=true

# Database Configuration
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=deep_learning_db
DATABASE_USER=postgres
DATABASE_PASSWORD=your_postgresql_password_here

# Cache Configuration (Redis/DragonflyDB)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=4
REDIS_PASSWORD=your_redis_password_here

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
ENABLE_MODEL_LOGGING=true
ENABLE_PREDICTION_LOGGING=true

# Security Configuration
ENABLE_AUTHENTICATION=false
API_KEY_HEADER=X-API-Key
ALLOWED_IPS=

# AI Services Configuration
# LiteLLM Configuration
LITELLM_ENABLED=true
LITELLM_API_BASE=
LITELLM_API_KEY=your_litellm_api_key_here
LITELLM_MODEL=gpt-3.5-turbo
LITELLM_MAX_TOKENS=4000
LITELLM_TEMPERATURE=0.7

# Anthropic Configuration
ANTHROPIC_ENABLED=false
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_MAX_TOKENS=4000

# OpenAI Configuration
OPENAI_ENABLED=false
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION=your_openai_organization_here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000

# Google AI Configuration
GOOGLE_AI_ENABLED=false
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
GOOGLE_AI_MODEL=gemini-pro
GOOGLE_AI_PROJECT_ID=your_google_project_id_here

# Memory Systems Configuration
# Letta Integration
LETTA_ENABLED=false
LETTA_API_BASE=http://localhost:8283
LETTA_API_KEY=your_letta_api_key_here
LETTA_USER_ID=default_user
LETTA_AGENT_ID=your_letta_agent_id_here
LETTA_TIMEOUT=30

# Weaviate Vector Database
WEAVIATE_ENABLED=false
WEAVIATE_URL=http://localhost:8080
WEAVIATE_API_KEY=your_weaviate_api_key_here
WEAVIATE_INDEX_NAME=DeepLearningMemory
WEAVIATE_VECTOR_DIMENSIONS=1536

# Langfuse Observability
LANGFUSE_ENABLED=false
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Model Architecture Configuration
# LSTM Configuration
LSTM_ENABLED=true
LSTM_HIDDEN_SIZE=128
LSTM_NUM_LAYERS=2
LSTM_DROPOUT=0.2
LSTM_BIDIRECTIONAL=true

# GRU Configuration
GRU_ENABLED=true
GRU_HIDDEN_SIZE=128
GRU_NUM_LAYERS=2
GRU_DROPOUT=0.2
GRU_BIDIRECTIONAL=false

# Transformer Configuration
TRANSFORMER_ENABLED=true
TRANSFORMER_D_MODEL=512
TRANSFORMER_NHEAD=8
TRANSFORMER_NUM_LAYERS=6
TRANSFORMER_DIM_FEEDFORWARD=2048
TRANSFORMER_DROPOUT=0.1

# CNN Configuration
CNN_ENABLED=true
CNN_NUM_FILTERS=64
CNN_FILTER_SIZES=3,4,5
CNN_DROPOUT=0.5
CNN_POOL_SIZE=2

# Training Configuration
TRAINING_BATCH_SIZE=32
TRAINING_EPOCHS=100
TRAINING_LEARNING_RATE=0.001
TRAINING_OPTIMIZER=adam
TRAINING_SCHEDULER=step
TRAINING_EARLY_STOPPING=true
TRAINING_PATIENCE=10
TRAINING_VALIDATION_SPLIT=0.2

# Pattern Discovery Configuration
PATTERN_ENSEMBLE_SIZE=5
PATTERN_CONFIDENCE_THRESHOLD=0.7
PATTERN_LENGTH=10
PATTERN_OVERLAP_THRESHOLD=0.5

# AI Coordination Settings
AI_INTELLIGENT_ROUTING=true
AI_FALLBACK_STRATEGY=cascade
AI_COORDINATION_TIMEOUT=30
AI_COORDINATION_RETRY_ATTEMPTS=3

# Feature Engineering Configuration
FEATURE_ENGINEERING_ENABLED=true
TECHNICAL_INDICATORS=sma,ema,rsi,macd,bollinger
LOOKBACK_PERIODS=5,10,20,50
VOLATILITY_WINDOWS=10,20,30

# Authentication Configuration
AUTH_ENABLED=false
JWT_SECRET_KEY=your_jwt_secret_key_here_minimum_32_characters
JWT_ALGORITHM=HS256
JWT_TOKEN_EXPIRE_MINUTES=60

# Service-to-Service Authentication Secrets
API_GATEWAY_SERVICE_SECRET=your_api_gateway_service_secret_here
DATABASE_SERVICE_SECRET=your_database_service_secret_here
TRADING_ENGINE_SERVICE_SECRET=your_trading_engine_service_secret_here
AI_ORCHESTRATION_SERVICE_SECRET=your_ai_orchestration_service_secret_here
DATA_BRIDGE_SERVICE_SECRET=your_data_bridge_service_secret_here

# Service Endpoints (optional overrides)
API_GATEWAY_URL=http://api-gateway:8000
DATABASE_SERVICE_URL=http://database-service:8008
TRADING_ENGINE_URL=http://trading-engine:8010
AI_ORCHESTRATION_URL=http://ai-orchestration:8004
DATA_BRIDGE_URL=http://data-bridge:8002

