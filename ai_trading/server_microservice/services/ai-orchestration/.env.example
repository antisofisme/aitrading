# AI Orchestration Service Environment Variables
# Copy this file to .env and set your actual values

# Service Configuration
MICROSERVICE_ENVIRONMENT=development
AI_ORCHESTRATION_PORT=8003
AI_ORCHESTRATION_DEBUG=false
AI_ORCHESTRATION_HOST=0.0.0.0

# CORS Configuration
CORS_ENABLED=true
CORS_ORIGINS=*

# Rate Limiting
RATE_LIMITING_ENABLED=true
RATE_LIMITING_REQUESTS_PER_MINUTE=1000

# AI Services Configuration
HANDIT_AI_ENABLED=true
HANDIT_AI_API_KEY=your_handit_ai_api_key_here
HANDIT_AI_BASE_URL=https://api.handit.ai
HANDIT_AI_TIMEOUT=30
HANDIT_AI_MAX_CONCURRENT=10

LETTA_MEMORY_ENABLED=true
LETTA_BASE_URL=http://localhost:8283
LETTA_API_KEY=your_letta_api_key_here
LETTA_MAX_MEMORIES=1000
LETTA_RETENTION_DAYS=30
LETTA_DEBUG=false

LANGGRAPH_ENABLED=true
LANGGRAPH_MAX_CONCURRENT=10
LANGGRAPH_TIMEOUT=300
LANGGRAPH_RETRIES=3
LANGGRAPH_DEBUG=false

LANGFUSE_ENABLED=true
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com
LANGFUSE_FLUSH_AT=15
LANGFUSE_FLUSH_INTERVAL=0.5
LANGFUSE_DEBUG=false
LANGFUSE_SESSION_TRACKING=true
LANGFUSE_USER_TRACKING=true

# Task Scheduling
TASK_SCHEDULING_ENABLED=true
TASK_SCHEDULING_MAX_CONCURRENT=50
TASK_SCHEDULING_QUEUE_SIZE=1000
TASK_SCHEDULING_RETRY_ATTEMPTS=3
TASK_SCHEDULING_RETRY_DELAY=5
TASK_SCHEDULING_TIMEOUT=3600

# Agent Coordinator
AGENT_COORDINATOR_ENABLED=true
AGENT_COORDINATOR_MAX_AGENTS=100
AGENT_COORDINATOR_MAX_QUEUE=1000
AGENT_COORDINATOR_TIMEOUT=300
AGENT_MESSAGE_TIMEOUT=30

# Workflow Orchestration
WORKFLOW_ORCHESTRATION_ENABLED=true
WORKFLOW_ORCHESTRATION_MAX_CONCURRENT=20
WORKFLOW_ORCHESTRATION_TIMEOUT=1800
WORKFLOW_ORCHESTRATION_CHECKPOINT_ENABLED=true
WORKFLOW_ORCHESTRATION_RECOVERY_ENABLED=true

# Observability
OBSERVABILITY_LEVEL=detailed
OBSERVABILITY_TRACE_ALL=true
OBSERVABILITY_LOG_AI_INTERACTIONS=true
OBSERVABILITY_PERFORMANCE_MONITORING=true
OBSERVABILITY_ERROR_TRACKING=true

# Performance
PERFORMANCE_ENABLE_CACHING=true
PERFORMANCE_CACHE_TTL_SECONDS=300
PERFORMANCE_ENABLE_COMPRESSION=true
PERFORMANCE_BATCH_PROCESSING=true
PERFORMANCE_PARALLEL_EXECUTION=true
PERFORMANCE_MEMORY_LIMIT_MB=2048

# Monitoring
HEALTH_CHECK_INTERVAL=30
METRICS_ENABLED=true
PERFORMANCE_TRACKING_ENABLED=true
MONITORING_COMPONENT_HEALTH=true
MONITORING_WORKFLOW_MONITORING=true
MONITORING_RESOURCE_MONITORING=true

# Security
ENABLE_AUTHENTICATION=false
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRY_HOURS=24
API_KEY_HEADER=X-API-Key
ALLOWED_IPS=
SESSION_TIMEOUT=3600
SECURITY_RATE_LIMIT_BY_USER=true

# Database
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=ai_orchestration_db
DATABASE_USER=postgres
DATABASE_PASSWORD=your_database_password_here

# Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=7
REDIS_PASSWORD=your_redis_password_here

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
ENABLE_AI_LOGGING=true
ENABLE_WORKFLOW_LOGGING=true
ENABLE_ORCHESTRATION_LOGGING=true
ENABLE_COMPONENT_LOGGING=true

# External Services
API_GATEWAY_URL=http://api-gateway:8000
TRADING_ENGINE_URL=http://trading-engine:8007
ML_PROCESSING_URL=http://ml-processing:8003
DEEP_LEARNING_URL=http://deep-learning:8004