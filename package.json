{
  "name": "aitrading",
  "version": "1.0.0",
  "description": "AI-powered trading platform with advanced diagram generation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "diagram:generate": "node dist/scripts/generate-diagram.js",
    "diagram:batch": "node dist/scripts/batch-generate.js",
    "diagram:watch": "node dist/scripts/watch-files.js",
    "claude-flow:init": "npx claude-flow@alpha hooks pre-task --description 'AI Trading Platform Initialization'",
    "claude-flow:status": "npx claude-flow@alpha swarm status",
    "claude-flow:memory": "npx claude-flow@alpha memory usage",
    "test:performance": "ts-node tests/performance/examples/RunPerformanceTests.ts",
    "test:performance:trading": "ts-node tests/performance/examples/RunPerformanceTests.ts trading",
    "test:performance:comprehensive": "ts-node tests/performance/examples/RunPerformanceTests.ts comprehensive",
    "test:performance:stress": "ts-node tests/performance/examples/RunPerformanceTests.ts stress",
    "test:performance:custom": "ts-node tests/performance/examples/RunPerformanceTests.ts custom",
    "test:performance:hft": "ts-node tests/performance/examples/RunPerformanceTests.ts stress",
    "test:coverage": "jest --coverage",
    "coverage:analyze": "node dist/coverage-system/cli/coverage-cli.js analyze",
    "coverage:monitor": "node dist/coverage-system/cli/coverage-cli.js monitor",
    "coverage:report": "node dist/coverage-system/cli/coverage-cli.js report",
    "coverage:trends": "node dist/coverage-system/cli/coverage-cli.js trends",
    "coverage:examples": "node dist/coverage-system/examples/basic-usage.js"
  },
  "keywords": [
    "trading",
    "ai",
    "diagrams",
    "mermaid",
    "claude-flow",
    "mcp",
    "visualization"
  ],
  "author": "AI Trading Platform Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "axios": "^1.12.2",
    "joi": "^18.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "ta-lib": "^0.11.0",
    "timeseries-analysis": "^1.0.12",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "mermaid": "^10.6.1",
    "puppeteer": "^21.5.2",
    "jsdom": "^23.0.1",
    "pdf-lib": "^1.17.1",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/jsdom": "^21.1.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "peerDependencies": {
    "claude-flow": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
